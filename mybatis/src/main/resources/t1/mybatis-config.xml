        <environment id=""  >         <!--环境变量-->
            <transactionManager type=""/>       <!--事物管理器-->
            <dataSource type=""  ></dataSource>   <!--数据源-->
        </environment>
    </environments>
    <databaseIdProvider type=""/>   <!--数据库厂商标识-->
    <mappers/>                       <!--映射器-->

</configuration>